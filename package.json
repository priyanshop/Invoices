{
  "name": "Invoices",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios --simulator='iPhone 13'",
    "lint": "eslint .",
    "start": "react-native start",
    "test": "jest",
    "pod": "cd ios && pod install && cd ..",
    "clean-ios": "cd ios && rm -rf build/ && pod install && cd ..",
    "clean-android": "cd android && ./gradlew clean && cd ..",
    "android-dev-bundle": "react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res",
    "debug-apk": "cd android && ./gradlew assembleDebug && cd ..",
    "release-apk": "cd android && ./gradlew assembleRelease && cd ..",
    "postinstall": "patch-package",
    "insert-code": "node insertCode.js"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^1.19.0",
